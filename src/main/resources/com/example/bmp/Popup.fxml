<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.text.TextFlow?>

<VBox alignment="TOP_RIGHT" prefHeight="106.0" prefWidth="369.0" stylesheets="@bootstrapfx.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bmp.Controlles.PopupController">

    <!-- Установите стиль "alert alert-success" из BootstrapFX -->
    <styleClass>
        alert alert-success
    </styleClass>

    <TextFlow fx:id="popupFlow" styleClass="alert" textAlignment="CENTER">
        <Text styleClass="strong" />
        <Text fx:id="popupText" />
    </TextFlow>
   <Button fx:id="okButton" mnemonicParsing="false" onAction="#okButtonClick" styleClass="btn-success" text="ОК">
      <VBox.margin>
         <Insets top="10.0" />
      </VBox.margin>
   </Button>
    <padding>
        <Insets bottom="10" left="10" right="10" top="10" />
    </padding>


</VBox>
